{
  "name": "kysely-firebird",
  "version": "1.1.0",
  "description": "Kysely Dialect and Type Generator for Firebird DB.",
  "author": "Ben Koppe",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/benkoppe/kysely-firebird"
  },
  "keywords": [
    "sql",
    "kysely",
    "dialect",
    "firebird",
    "firebirddb"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "test": "vitest",
    "release": "pnpm build && changeset publish",
    "dev": "pnpm lint && node dist/dev.js"
  },
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "kysely": "^0.28.2",
    "node-firebird": "github:hgourvest/node-firebird#e1c4dd9",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^22.15.29",
    "kysely-firebird": "link:",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
